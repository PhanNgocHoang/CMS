<%-include ('../layouts/header.ejs')-%>

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Manage Class
            <small>Control panel</small>
        </h1>
    </section>

<!-- Main content -->
<section class="content">
    <h1>Class</h1>
    <a href="#" class="btn btn-primary">
       <i class="fa fa-plus"></i> Create
    </a>
    <br />
    <div class="table-responsive">
        <table class="table table-striped table-hover">
            <tr>
                <th>Class ID</th>
                <th>Class Name</th>
                <th>Created_at</th>
                <th>Updated at</th>
            </tr>
            <tr>
                <%for(x in data.class){%>
                <td>
                    <%=data.class[x].Class_ID%>
                </td>
                <td>
                    <%=data.class[x].Class_name%>
                </td>
                <td>
                    <%=data.class[x].Create_at%>
                </td>
                <td>
                    <%=data.class[x].Update_at%>
                </td>
                <td>
                    <a href="#" title="ViewExercise">
                        <i class="fa fa-eye"> View Exercise</i>
                    </a>
                </td>
                <td>
                    <a href="#" title="Detail">
                        <i class="fa fa-eye"></i>
                    </a> &nbsp;
                    <a href="#" title="Update">
                        <i class="fa fa-pencil"></i>
                    </a> &nbsp;
                    <a href="#" title="Delete" onclick="return confirm('Are you sure delete?')">
                        <i class="fa fa-trash"></i>
                    </a>
                </td>
                <%}%>
            </tr>
        </table>
    </div>
</section>
<!-- /.content -->
</div>
<!-- /.content-wrapper -->

<%-include ('../layouts/footer.ejs')-%>