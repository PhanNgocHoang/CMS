<%-include ('../layouts/header.ejs')-%>
<div class="content-wrapper">
   <!-- Content Header (Page header) -->
   <!-- Main content -->
   <main class="main-container container">
       <div class="view-account">
           <section class="module">
               <div class="module-inner">
                  <h3 class="fieldset-title">ROLE: <%=User_info.Role_Name%></h3>
                   <div class="content-panel">
                     <div id = "personal_tutor" style = "width: 550px; height: 400px; margin: 0 auto">
                     </div>
                   </div>
               </div>
           </section>
           <table id="name">
         </table>
       </div>
   </main>
   <!-- /.content -->
</div>
<script type = "text/javascript" src = "https://www.gstatic.com/charts/loader.js">
</script>
<script type = "text/javascript">
   google.charts.load('current', {packages: ['corechart']});
</script>
<script>
   function Chart() {
      // Define the chart to be drawn.
      var data = google.visualization.arrayToDataTable([
         ['Message', 'Send', 'Receive'],
         ['Personal tutor 1', 3, 2],
         ['Personal tutor 2', 2, 3],
         ['Personal tutor 3', 1, 5],
         ['Personal tutor 4', 3, 9],
         ['Personal tutor 4', 4, 2]
      ]);

      // Set chart options
      var options = {
         title : 'Message',
         vAxis: {title: 'Message'},
         seriesType: 'bars',
      };

      // Instantiate and draw the chart.
      var chart = new google.visualization.ComboChart(document.getElementById('personal_tutor'));
      chart.draw(data, options);
   }
   google.charts.setOnLoadCallback(Chart);
</script>
